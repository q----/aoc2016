cpy a d

cpy 7 c
cpy 362 b ;jnz c -5
inc d ;jnz b -2
dec b
jnz b -2
dec c
jnz c -5

; d = a + 7 * 362




cpy d a ;jnz 1 -21
jnz 0 0 ;jnz a -19
cpy a b


cpy 0 a
cpy 2 c ;jnz 1 -7
jnz b 2 ;jnz c -4
jnz 1 6
dec b   ;jnz b 2
dec c
jnz c -4 ;if c = 0
inc a
jnz 1 -7

; a = b/2(ish) c = 2-(b % 2)

; c = 2 --> output 0
; c = 1 --> output 1
cpy 2 b
jnz c 2
jnz 1 4
dec b
dec c
jnz 1 -4
jnz 0 0
out b
jnz a -19
jnz 1 -21

; 
